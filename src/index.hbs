        crossorigin="anonymous"></script>
    <script>
        /**
        *  Set min-height for each domein to the viewport-height and reset it on window-resize
        */
        var setDomeinHeight = function () {
            var windowH = $(window).height() + 'px';
            $('.domein').each(function () {
                $(this).css({ 'min-height': windowH });
            })
        }(); // immediately invoke this function

        // re-invoke on window resize
        $(window).resize(function (event) {
            setDomeinHeight();
        });

        /**
        *  Autoscroll to correct section when navbar button is clicked
        */
        $('a').click(function () {
            $('html, body').animate({
                scrollTop: $($.attr(this, 'href')).offset().top
            }, 2000);
            return false;
        });
    </script>
